COMMENT validate equipment_ui_list (dom)
GOTO /admin/login/
WAIT SELECTOR input#id_username
FILL input#id_username admin
FILL input#id_password Admin!2345
CLICK input[type=submit]
WAIT URL CONTAINS /admin/
GOTO /admin/maintenance/equipment/_direct/
WAIT SELECTOR css=:is(form#changelist-form,table#result_list,#content-main,#content,body)
AUTOVALIDATE key=equipment_ui_list baseline=targets\reference\equipment_ui_list.png words_recall=0.9 live_source=dom alert_dir=_otokodlama\alerts min_token_len=3 ignore_numbers=yes ignore_patterns=csrf,token,\d{2,},page_\d+,id_\d+,ts_\d+

