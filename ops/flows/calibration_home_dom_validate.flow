COMMENT validate calibration_home with DOM tokens
GOTO /admin/login/
WAIT SELECTOR input#id_username
FILL input#id_username admin
FILL input#id_password Admin!2345
CLICK input[type=submit]
WAIT URL CONTAINS /admin/
GOTO /admin/maintenance/calibration/
WAIT SELECTOR #content-main
AUTOVALIDATE key=calibration_home baseline=targets/reference/calibration_home.png words_recall=0.90 live_source=dom alert_dir=_otokodlama/alerts min_token_len=3 ignore_numbers=yes ignore_patterns=csrf,token,\d{2,},page_\d+,id_\d+,ts_\d+,ver_\d+
