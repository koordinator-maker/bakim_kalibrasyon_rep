# Admin -> Plan sayfası (doğrudan URL)
GOTO /admin/login/
WAIT SELECTOR input#id_username
FILL input#id_username admin
FILL input#id_password Admin!2345
CLICK input[type=submit]
WAIT URL CONTAINS /admin/
WAIT SELECTOR #content-main

# Burayı bulduğun gerçek URL ile değiştir
GOTO /admin/maintenance/plan/
WAIT SELECTOR h1
SCREENSHOT targets/screens/admin-plan.png
EXPECT URL CONTAINS /admin/maintenance/plan/
